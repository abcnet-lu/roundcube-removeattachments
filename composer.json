{
    "name": "dsoares/removeattachments",
    "type": "roundcube-plugin",
    "description": "This plugin adds an option to remove one or more attachments from a message",
    "keywords": ["mail","roundcube","attachments"],
    "homepage": "https://github.com/dsoares/roundcube-removeattachments",
    "version": "0.3.0",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Diana Soares",
            "email": "diana.soares@gmail.com",
            "homepage": "https://github.com/dsoares",
            "role": "Developer"
        },
        {
            "name": "Philip Weir",
            "email": "roundcube@tehinterweb.co.uk",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://plugins.roundcube.net"
        }
    ],
    "require": {
        "php": ">=5.4",
        "roundcube/plugin-installer": ">=0.1.3"
    },
    "scripts": {
        "lint": "php-cs-fixer -v fix --diff --diff-format udiff --dry-run",
        "fix": "php-cs-fixer fix"
    },
    "extra": {
        "roundcube": {
            "min-version": "1.1"
        }
    }
}
